# Database Configuration
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@postgresql:5432/lobechat
DATABASE_DRIVER=node
POSTGRES_PASSWORD=YOUR_PASSWORD

# Service Mode
NEXT_PUBLIC_SERVICE_MODE=server
NEXT_PUBLIC_ENABLE_NEXT_AUTH=1

# Application URLs
APP_URL=http://localhost:3010
NEXTAUTH_URL=http://localhost:3010/api/auth

# Authentication Secrets
NEXT_AUTH_SECRET=GENERATE_A_RANDOM_SECRET_HERE
NEXT_AUTH_SSO_PROVIDERS=casdoor
KEY_VAULTS_SECRET=GENERATE_A_BASE64_KEY_HERE

# Casdoor Configuration
AUTH_CASDOOR_ISSUER=http://casdoor:8000
AUTH_CASDOOR_ID=YOUR_CASDOOR_APP_ID
AUTH_CASDOOR_SECRET=YOUR_CASDOOR_APP_SECRET

# MinIO/S3 Configuration
S3_ENDPOINT=http://minio:9000
S3_PUBLIC_DOMAIN=http://localhost:9000
S3_ACCESS_KEY_ID=YOUR_MINIO_ACCESS_KEY
S3_SECRET_ACCESS_KEY=YOUR_MINIO_SECRET_KEY
S3_BUCKET=lobe
S3_ENABLE_PATH_STYLE=1
S3_SET_ACL=0

# LLM Configuration
LLM_VISION_IMAGE_USE_BASE64=1

# Search Engine
SEARXNG_URL=http://searxng:8080

# AI Provider API Keys (Optional - add your keys here)
OPENAI_API_KEY=sk-YOUR_OPENAI_API_KEY
OPENAI_PROXY_URL=https://api.openai.com/v1
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY
MISTRAL_API_KEY=YOUR_MISTRAL_API_KEY
DEEPSEEK_API_KEY=YOUR_DEEPSEEK_API_KEY
XAI_API_KEY=YOUR_XAI_API_KEY

